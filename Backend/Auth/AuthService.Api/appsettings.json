{
  "Logging": {
    "LogSuccessfulIncomingHttpRequestBodies": true,
    "LogSuccessfulOutgoingHttpRequestBodies": true
  },
  "Serilog": {
    "LevelSwitches": {
      "$controlSwitch": "Information"
    },
    "MinimumLevel": {
      "ControlledBy": "$controlSwitch",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Information",
        "System.Net.Http.HttpClient": "Warning",
        "Hangfire": "Warning"
      }
    },
    "WriteTo:ConditionalSeq": {
      "Name": "Conditional",
      "Args": {
        "expression": "false",
        "configureSink": [
          {
            "Name": "Seq",
            "Args": {
              "controlLevelSwitch": "$controlSwitch",
              "serverUrl": "http://seq:5341"
            }
          }
        ]
      }
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=host.docker.internal;Port=5433;Database=kwetter.AuthService;User Id=postgres;Password=postgres;"
  }
}
